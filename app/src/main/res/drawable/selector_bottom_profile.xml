<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <item android:id="@+id/state_on" android:drawable="@drawable/ic_active_profile" android:state_checked="true" />

    <item android:id="@+id/state_off" android:drawable="@drawable/ic_inactive_profile" />


    <transition android:drawable="@drawable/ic_active_profile" android:fromId="@id/state_off" android:toId="@id/state_on" tools:ignore="StateListReachable" />
</selector>